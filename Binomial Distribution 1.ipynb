    "from math import factorial as f\n",
    "b, g = map(float, input().split())\n",
    "def comb(n,r):\n",
    "    return(f(n) / (f(r) * f(n-r)))\n",
    "p = b/(b+g)\n",
    "n = 6\n",
    "print(round(sum((p**k) * (1-p)**(n-k) * comb(n,k) for k in range(3,n+1) ),3))"
   ]
  }
